{
  "name": "kolomyia-elder-map",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build+preview": "tsc && vite build && vite preview",
    "deploy": "tsc && vite build && npx gh-pages -d dist",
    "upgrade": "npx npm-check-updates --upgrade && npm install"
  },
  "dependencies": {
    "@maptiler/leaflet-maptilersdk": "^4.1.0",
    "leaflet": "^1.9.4",
    "leaflet-hash": "^0.2.1",
    "leaflet-providers": "^2.0.0",
    "react": "^19.1.0",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "^19.1.0",
    "react-leaflet": "^5.0.0",
    "reactjs-popup": "^2.0.6"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.18",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-react": "^4.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}
