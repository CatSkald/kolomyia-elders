{
  "name": "kolomyia-elder-map",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build+preview": "tsc && vite build && vite preview",
    "deploy": "tsc && vite build && npx gh-pages -d dist",
    "upgrade": "npx npm-check-updates --upgrade && npm install"
  },
  "dependencies": {
    "@maptiler/leaflet-maptilersdk": "^4.1.1",
    "leaflet": "^1.9.4",
    "leaflet-hash": "^0.2.1",
    "leaflet-providers": "^3.0.0",
    "react": "^19.2.4",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "^19.2.4",
    "react-leaflet": "^5.0.0",
    "react-string-replace": "^2.0.1",
    "reactjs-popup": "^2.0.6"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.21",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^9.39.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
